#include <stdio.h>

int main() {
    int a[10], i, n;
    float sum = 0, avg;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        sum += a[i];
    }

    avg = sum / n;
    printf("Average = %.2f\n", avg);

    return 0;
}
